  <!DOCTYPE html>
  <html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="shortcut icon" href="assets/ico/favicon.ico">
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
    <style type="text/css">
    </style>
    <title>Quiz</title>

  <link href="assets/css/bootstrap.css" rel="stylesheet">

  <link href="assets/css/style.css" rel="stylesheet">
  <link href="assets/css/font-awesome.min.css" rel="stylesheet">
  
  <script src="assets/js/modernizr.js"></script>
</head>
<body>
  <div id="blue" style="margin-bottom: 10px;">
    <div class="container">
      <div class="row">
        <h1 style="color: #fff">QUIZ - MEDIUM
        </h1>
      </div>
    </div> 
  </div>
  <div class="container" style="font-size: 20px;margin-top: 20px;">
      <div class="row" style="margin-top: 34px;">
    <form method="GET" action="CheckQuiz">

        <div class="col-md-9 col-md-offset-2" >
          <table class="table table-hover table-condensed">
            <% for(i=10,j=0;i<20;i++,j++){ %>
            <thead>
              <tr>
                <th colspan="2"><%= i+1%>. <%= results[i].question %></th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td>          
                  <div class="radio">
                    <label class="radio"><input type="radio" name="correctAnswer<%= [j]%>" value="<%= results[i].option1 %>"><%= results[i].option1 %></input></label>
                  </div>
                </td>
                <td>
                  <div class="radio">
                    <label class="radio"><input type="radio" name="correctAnswer<%= [j]%>" value="<%= results[i].option2 %>"><%= results[i].option2 %></input></label>
                  </div>
                </td>
              </tr>
              <tr>
                <td>
                  <div class="radio">
                    <label class="radio"><input type="radio" name="correctAnswer<%= [j]%>" value="<%= results[i].option3 %>"><%= results[i].option3 %></input></label>
                  </div>
                </td>
                <td>
                  <div class="radio">
                    <label class="radio"><input type="radio" name="correctAnswer<%= [j]%>" value="<%= results[i].option4 %>"><%= results[i].option4 %></input></label>
                  </div>
                </td>
              </tr>
            </tbody>
          <% }%>
          </table>
        </div>
          <div class="col-md-12" align="center">
            <button id="submit" class="btn btn-info">
              Submit
            </button>
          </div>
      </form>
    </div>
  </body>
</html>

            

